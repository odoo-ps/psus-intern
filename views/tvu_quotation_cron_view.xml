<?xml version="1.0" encoding='UTF-8'?>
<odoo>
    <data noupdate="1">
        <record id="autocancel_cron_job_tvu" model="ir.cron">
        <field name="name">TVU Quotation Sanity Cron JOB</field>
        <field name="model_id" ref="sale.model_sale_order"/>
        <field name="state">code</field>
        <field name="code">model.cancel_expired_quotations()</field>
        <field name="nextcall" eval="(DateTime.now() + timedelta(days=0)).strftime('%Y-%m-%d 04:00:00')" />
        <field name='interval_number'>1</field>
        <field name='interval_type'>days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
    </record>
    </data>
</odoo>
