<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="3pl_export_to_edi_action" model="ir.actions.server" >
        <field name="name">Export to 3PL</field>
        <field name="model_id" ref="stock.model_stock_picking"/>
        <field name="binding_model_id" ref="stock.model_stock_picking" />
        <field name="state">code</field>
        <field name="code">
if records:
    records.with_context(force_export=True).export_pickings_to_edi()
        </field>
    </record>

    <record id="3pl_import_from_edi_action" model="ir.actions.server">
        <field name="name">Import from 3PL</field>
        <field name="model_id" ref="stock.model_stock_picking"/>
        <field name="binding_model_id" ref="stock.model_stock_picking" />
        <field name="state">code</field>
        <field name="code">model.import_pickings_from_edi()</field>
    </record>

