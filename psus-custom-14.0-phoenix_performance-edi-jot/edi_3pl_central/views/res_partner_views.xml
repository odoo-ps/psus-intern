<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="phoenix_partner_form" model="ir.ui.view">
        <field name="name">Phoenix Performance: Contact Form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='property_delivery_carrier_id']" position="after">
                <field name="shipping_collect_account"/>
                <field name="ship_priority"/>
                <field name="warehouse_instructions"/>
              </xpath>
        </field>
    </record>
</odoo>