<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <record id="res_partner_views_inherit" model="ir.ui.view">
        <field name="name">res.partner.views.inherit</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"></field>
        <field name="arch" type="xml">
            <xpath expr="//form//sheet//notebook//page[last()]" position="after">
                <page name="e_commerce" string="eCommerce">
                    <group>
                        <field name="product_list_id"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
