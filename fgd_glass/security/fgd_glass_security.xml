<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="group_fgd_sales_own" model="res.groups">
        <field name="name">FGD Sales Own</field>
    </record>

    <record id="group_fgd_sales_admin"  model="res.groups"> 
        <field name="name">FGD Sales Admin</field>
        <field name="implied_ids" eval="[(4, ref('fgd_glass.group_fgd_sales_own'))]"/>
    </record>

    <record id="group_fgd_sales_admin_commission"  model="res.groups"> 
        <field name="name">FGD Sales Admin (Commission)</field>
        <field name="implied_ids" eval="[(4, ref('fgd_glass.group_fgd_sales_own')), (4, ref('fgd_glass.group_fgd_sales_admin'))]" />
    </record>

    <record id="group_fgd_sales_all"  model="res.groups">
        <field name="name">FGD Sales All</field>
        <field name="implied_ids" eval="[(4, ref('fgd_glass.group_fgd_sales_own'))]" />
    </record>
</odoo>