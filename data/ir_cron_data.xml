<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="ir_cron_module_expiry_check" model="ir.cron">
            <field name="name">Expired Quotations Check</field>
            <field name="model_id" ref="model_sale_tvu_expiry_expiry_check"/>
            <field name="code">model.check_records()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 12:0:0')" />
            <field eval="False" name="doall" />
            <field name="priority">5</field>
        </record>


    </data>
</odoo>
