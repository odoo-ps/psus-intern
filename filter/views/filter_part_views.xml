<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="filter_part_view_tree" model="ir.ui.view">
        <field name="name">filter.part.view.tree</field>
        <field name="model">parts.part</field>
        <field name="arch" type="xml">
            <tree string="Parts">
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id="filter_part_view_form" model="ir.ui.view">
        <field name="name">filter.part.view.form</field>
        <field name="model">parts.part</field>
        <field name="arch" type="xml">
            <form string="Part">
                <field name="name" />
            </form>
        </field>
    </record>

    <record id="filter_part_action" model="ir.actions.act_window">
        <field name="name">All Parts</field>
        <field name="res_model">parts.part</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>