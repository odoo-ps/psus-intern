<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="session_view_form">
            <field name="name">product.productlist.form</field>
            <field name="model">product.list</field>
            <field name="arch" type="xml">
                <form string="Product list Form">
                    <sheet>
                        <h1>
                            <field name="name" />
                        </h1>
                        <group>
                            <notebook>
                                <page string="Product List">
                                    <field name="product_ids" />
                                </page>
                                <page string="Customers That Use This List">
                                    <field name="customer_id" />
                                </page>
                            </notebook>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>
