<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="product_lists_view_form">
            <field name="name">greenville.products.lists.form</field>
            <field name="model">greenville.products_list</field>
            <field name="arch" type="xml">
                <form string="Product Lists Form">
                    <sheet>
                        <h1>
                            <field name="name" string="Name"/>
                        </h1>
                        <notebook>
                            <page string="Product List">
                                <field name="product_ids" string="Product Template"/>
                            </page>
                            <page string="Customers That Use This List">
                                <field name="customer_ids"/>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>