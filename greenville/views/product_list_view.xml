<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="product_list_view_form" model="ir.ui.view" >
            <field name="name">product.list.view.form</field>
            <field name="model">product.list</field>
            <field name="arch" type="xml">
                <form string="Product list form">
                    <sheet>
                        <h1>
                            <field name="name" />
                        </h1>
                        <notebook>
                            <page string="Product list">
                                <field name="products_ids" />
                            </page>
                            <page string="Customers that use this list">
                                <field name="costumers_ids" />
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="product_list_view_search" model="ir.ui.view">
            <field name="name">product.list.view.search</field>
            <field name="model">product.list</field>
            <field name="arch" type="xml">
                <search>
                    <field name="name" />
                </search>
            </field>
        </record>
</odoo>
