<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="view_partner_form_inherit_product_list">
            <field name="name">res.partner.form.inherit.product.list</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//sheet//notebook" position="inside">
                    <page string="eCommerce">
                        <group>
                            <field name="product_list_id"></field>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
