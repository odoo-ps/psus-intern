<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_" model="ir.cron">
            <field name="name">Auto Cancel The Expired Quotations</field>
            <field name="model_id" ref="model_sale_order"/>
            <field name="state">code</field>
            <field name="code">model.auto_cancel_expired_quotations()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 13:00:00')" />
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
        </record>
     </data>
</odoo>
