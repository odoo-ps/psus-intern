<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="auto_cancel_quotations_TVUNetworks" model="ir.cron">
            <field name="name">Auto_cancellation</field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall">0</field>
            <field name="nextcall" eval = "(datetime.now().replace(hour=0,minute=0)+ timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')" />
            <field name="model_id" ref="sale.model_sale_ordr" />
            <field name="function" eval="Cancellation()" />
            <field name="args" eval="" />


        </record>
    </data>
</odoo>